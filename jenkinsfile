pipeline {
    agent any

    options {
        skipDefaultCheckout(false)
    }

    stages {
        stage('Checkout') {
            steps {
                deleteDir()
                checkout scm
            }
        }

        stage('Debug') {
            steps {
                bat 'dir /s'
            }
        }

        stage('Restore') {
            steps {
                bat 'dotnet restore gestion_stages.sln'
            }
        }
    }
}
